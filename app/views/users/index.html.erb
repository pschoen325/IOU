<% page_header "Users" %>

<%= will_paginate %>
<div class="container">
  <div class="page_header">
    <h1>iOU</h1>
    <h3>Users</h3>
  </div>
</div>
<ul>
  <% @users.each do |user| %>
    <li>
      <strong><%= user.name %></strong>
      <% unless current_user == user %>
        <%= link_to 'Send message', new_message_path(to: user.id), class: 'btn btn-default btn-sm' %>
      <% end %>
    </li>
  <% end %>
</ul>

<%= will_paginate %>
